{
    "name": "youdao-translator",
    "displayName": "Youdao translator",
    "description": "Translate English to Chinese with Youdao.",
    "version": "0.10.7",
    "icon": "img/icon.png",
    "publisher": "huren",
    "engines": {
        "vscode": "^1.18.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.youdaoTranslate"
    ],
    "main": "./extension",
    "contributes": {
        "keybindings": [
            {
                "command": "extension.youdaoTranslate",
                "key": "meta+t",
                "mac": "cmd+shift+t",
                "when": "editorTextFocus"
            }
        ],
        "commands": [
            {
                "command": "extension.youdaoTranslate",
                "title": "Youdao Translate"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/huturen/youdao-translator.git"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "eslint": "^4.6.1",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "request": "^2.81.0"
    }
}
